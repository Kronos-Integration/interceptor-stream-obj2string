{
  "name": "kronos-interceptor-stream-obj2string",
  "version": "0.0.0-semantic-release",
  "main": "dist/stream2object-interceptor.js",
  "module": "src/stream2object-interceptor.js",
  "description": "Splits a stream into lines.",
  "keywords": [
    "stream",
    "line",
    "parser",
    "kronos-interceptor"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "documentation readme src/stream2object-interceptor.js --section=API",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "lint": "documentation lint src/stream2object-interceptor.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "kronos-interceptor": "^2.5.2"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "kronos-test-interceptor": "^3.1.1",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.7.3",
    "rollup": "^0.58.2",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^15.4.1",
    "documentation": "^6.3.2",
    "travis-deploy-once": "^5.0.0"
  },
  "engines": {
    "node": ">=8.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-stream-obj2string.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-stream-obj2string/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor-stream-obj2string#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
