    "docs": "documentation readme ./src/stream2object-interceptor.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/stream2object-interceptor.mjs"
  },
  "dependencies": {
    "@kronos-integration/interceptor": "^10.0.11"
